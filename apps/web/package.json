{
  "name": "@suptia/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "type-check": "npm run typecheck",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "next lint",
    "format": "prettier --write .",
    "format:check": "prettier . --check",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "test:fix": "vitest run --reporter=verbose",
    "headers": "cd ../.. && node scripts/check-headers.mjs",
    "jsonld": "cd ../.. && node scripts/validate-jsonld.mjs",
    "dod:check": "cd ../.. && node scripts/check-dod.mjs",
    "preview:verify": "node ../../scripts/verify-preview-env.mjs",
    "preview:url": "node ../../scripts/get-preview-url.mjs",
    "perf:optimize": "node ../../scripts/performance-optimizer.mjs",
    "perf:report": "node ../../scripts/performance-optimizer.mjs --report",
    "perf:cleanup": "node ../../scripts/performance-optimizer.mjs --cleanup"
  },
  "dependencies": {
    "@sanity/client": "^7.11.2",
    "@sanity/vision": "^4.10.1",
    "chalk": "^5.6.2",
    "groq": "^4.10.1",
    "next": "^14.2.33",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "^4.10.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.4"
  }
}
