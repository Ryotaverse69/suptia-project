{
  "name": "sanity-bulk-import",
  "version": "1.0.0",
  "description": "Suptia用 Sanity一括インポートツール（MVP版）",
  "type": "module",
  "main": "src/importer.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "import": "node src/importer.js",
    "import:dry": "DRY_RUN=true node src/importer.js",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "sanity",
    "import",
    "bulk",
    "suptia"
  ],
