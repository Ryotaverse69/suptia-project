  "license": "MIT",
  "dependencies": {
    "@sanity/client": "^6.22.6",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.7",
    "fast-glob": "^3.3.2",
    "ora": "^8.1.1",
    "p-map": "^7.0.2",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/proper-lockfile": "^4.1.4",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
